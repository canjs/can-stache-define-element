  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "can-type": "^0.1.3",
    "can-view-callbacks": "^4.4.0",
    "can-view-scope": "^4.13.1",
    "detect-cyclic-packages": "^1.1.0",
    "fixpack": "^2.3.1",
    "http-server": "^0.11.1",
    "jshint": "^2.9.1",
    "steal": "^2.2.1",
    "steal-qunit": "^2.0.0",
    "test-saucelabs": "0.0.6",
    "testee": "^0.9.0"
